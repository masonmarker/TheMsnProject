
function(@_r='min_sort', ret(_r, (
    
    @ start = 0,
    @ end = -(len(nums), 1),

    while (start.less(end), (
        @ mid = //(+(start, end), 2),

        if (greater(nums.get(mid), nums.get(end)),
            start.add(+(mid, 1)),
        if (less(nums.get(mid), nums.get(start)),
            end.set(mid),
        end.sub(1)))

    )),
    nums.get(start)
)), 'nums')

print(min_sort([1, 3, 5]))
print(min_sort([2, 2, 2, 0, 1]))