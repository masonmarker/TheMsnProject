# element discovery
import ('lib/auto/discover')

# starts Google Chrome
@ chrome = app('C:\\Program Files\\Google\\Chrome\\Application\\chrome.exe')
chrome.start()
chrome.write(input('type a website: '))
chrome.enter()


discover_click(chrome, 45, script(
    and(greater(len(child.children()), 5), 
        less(len(child.children()), 10)
    )
))



chrome.stop()