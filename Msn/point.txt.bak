:: point class;
:: author : Mason Marker;
:: date   : 03/18/2022;

object point;
i xval = 0;
i yval = 0;

f up;
up = point#yval += 1;
end up;

f down;
down = point#yval -= 1;
end down;

f right;
right = point#xval += 1;
end right;

f left;
left = point#xval -= 1;
end left;

f nw;
nw = point#up;
nw = point#left;
end nw;

f ne;
ne = point#up;
ne = point#right;
end ne;

f sw;
sw = point#down;
sw = point#left;
end sw;

f se;
se = point#down;
se = point#right;
end se;

f prnt;
prnt = println ( point#xval , point#yval ); 
end prnt;

point has xval yval up down left right nw ne sw se prnt;